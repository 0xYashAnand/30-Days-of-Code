#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int t;
    cin>>t;
    for (int i =0; i<t; i++) {
    string str;
    cin>>str;
    int y = str.length();
    for (int j = 0; j<y; j++) {
    cout<<str[j];
    j++;
    }
    cout<<" ";
    for (int k = 1; k<y; k++) {
    cout<<str[k];
    k++;
    }
    cout<<endl;
    }  
    return 0;
}
